<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.alvaro.onlineshopping">
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- Customer -->
        <activity android:name=".Customer.OptionsCustomer">
        </activity>
        <activity android:name=".Customer.ListarCustomer">
        </activity>
        <activity android:name=".Customer.CrearCustomer">
        </activity>
        <activity android:name=".Customer.ViewCustomer">
        </activity>
        <activity android:name=".Customer.EditarCustomer">
        </activity>
        <!-- WebUser -->
        <activity android:name=".WebUser.OptionsWebuser">
        </activity>
        <activity android:name=".WebUser.ListarWebuser">
        </activity>
        <activity android:name=".WebUser.CrearWebuser">
        </activity>
        <activity android:name=".WebUser.ViewWebuser">
        </activity>
        <activity android:name=".WebUser.EditarWebuser">
        </activity>
        <!-- Account -->
        <activity android:name=".Account.OptionsAccount">
        </activity>
        <activity android:name=".Account.ListarAccount">
        </activity>
        <activity android:name=".Account.CrearAccount">
        </activity>
        <activity android:name=".Account.ViewAccount">
        </activity>
        <activity android:name=".Account.EditarAccount">
        </activity>

    </application>

</manifest>